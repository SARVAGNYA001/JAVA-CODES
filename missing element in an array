import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt();
		int[]arr=new int[n];
		for(int i=0;i<n;i++){
		    arr[i]=sc.nextInt();
		}
		int temp=1;
		for(int i=0;i<n;i++){
		    if(arr[i]!=i+temp){
		        System.out.println("missing element is:");
		    System.out.println(i+temp);
		    temp++;
		    continue;
		}
		}
	}
}
	
