import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int m=sc.nextInt();
		int max=Math.max(n,m);
		int count=0;
		for(int i=1;i<max;i++){
		    if(n%i==0 && m%i==0){
		        count=i;
		    }
		}
		System.out.println(count);
	}
}
